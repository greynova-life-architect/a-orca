{
  "name": "a-orca",
  "version": "1.0.0",
  "description": "Project management with Cursor CLI integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently -n api,vite \"node server.js\" \"vite\"",
    "dev:vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "node --test tests/*.test.js",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write \"**/*.js\" \"**/*.jsx\""
  },
  "dependencies": {
    "better-sqlite3": "^11.6.0",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "vite": "^5.0.0"
  }
}
